<?xml version="1.0" encoding="utf-8"?>
<paths xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- 自定义目录映射用于分享,可以定义多个。name值随意填写，path需要填写子目录名,
    要和文件保存目录保持一致,获得路径时用context.getExternalFilesDir("share")，context.getFilesDir() -->
    <external-files-path name="sharedata" path="share" />
    <external-files-path name="sharedata3" path="." />
    <files-path name="sharedata2" path="." />
</paths>