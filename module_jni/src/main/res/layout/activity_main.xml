<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <Button
        android:id="@+id/btn_str2cpp"
        android:text="JAVA传递字符串"
        android:layout_gravity="center_horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"></Button>

    <Button
            android:id="@+id/btn_intArr2cpp"
            android:text="JAVA传递int数组"
            android:layout_gravity="center_horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"></Button>
    <Button
            android:id="@+id/btn_strArr2cpp"
            android:text="JAVA传递string数组"
            android:layout_gravity="center_horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"></Button>

    <Button
            android:id="@+id/btn_cppCallJavaStatic"
            android:text="c++调java静态方法"
            android:layout_gravity="center_horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"></Button>

    <Button
            android:id="@+id/btn_cppGetJavaStatic"
            android:text="c++调java静态属性"
            android:layout_gravity="center_horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"></Button>


    <Button
            android:id="@+id/btn_cppGetJavaObj"
            android:text="c++调java实例属性方法"
            android:layout_gravity="center_horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"></Button>
</LinearLayout>